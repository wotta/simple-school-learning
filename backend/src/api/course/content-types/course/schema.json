{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "courses",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "end_date": {
      "type": "date"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "questionaire.questionnaire",
        "questionaire.story"
      ]
    },
    "subject": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::subject.subject",
      "inversedBy": "courses"
    },
    "description": {
      "type": "text"
    },
    "amount_questions": {
      "type": "integer",
      "default": 5,
      "min": 3,
      "max": 10
    }
  }
}
